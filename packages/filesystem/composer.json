{
    "name": "rosem/filesystem",
    "description": "Filesystem library",
    "keywords": [
        "rosem",
        "file",
        "filesystem"
    ],
    "homepage": "https://rosem.space/filesystem",
    "license": "MIT",
    "authors": [
        {
            "name": "roshecode",
            "email": "iroman.via@gmail.com",
            "homepage": "https://rosem.space",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/console": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Rosem\\Component\\Filesystem\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rosem\\Component\\Filesystem\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
